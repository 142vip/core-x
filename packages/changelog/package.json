{
  "name": "@142vip/changelog",
  "type": "module",
  "version": "0.0.1-alpha.14",
  "private": false,
  "description": "基于Git提交记录，生成漂亮的变更日志，输出Markdown格式的CHANGELOG.md文件，更加人性化",
  "author": "mmdapl <mmdapl@163.com>",
  "license": "MIT",
  "homepage": "https://142vip.github.io/core-x/packages/changelog/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/142vip/core-x.git",
    "directory": "packages/changelog"
  },
  "keywords": [
    "公众号搜：储凡",
    "142vip",
    "@142vip",
    "@142vip/changelog",
    "github",
    "release",
    "releases",
    "conventional",
    "changelog"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "changelog": "./cli.mjs"
  },
  "files": [
    "*.mjs",
    "dist"
  ],
  "authorInfo": {
    "name": "微信公众号：储凡",
    "email": "fairy_vip@2925.com",
    "url": "https://github.com/142vip",
    "homePage": "https://142vip.cn"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@142vip/utils": "workspace:*",
    "@esm2cjs/execa": "6.1.1-cjs.1",
    "c12": "2.0.1",
    "changelogen": "0.5.5",
    "convert-gitmoji": "0.1.5",
    "ofetch": "1.3.4",
    "semver": "7.7.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
