{
  "name": "@142vip/grpc",
  "version": "0.0.1-alpha.1",
  "private": false,
  "description": "Grpc工具包，支持proto文件加载、解析，Grpc客户端、服务端数据连接交互，支持健康检查",
  "author": "mmdapl <mmdapl@163.com>",
  "license": "MIT",
  "homepage": "https://142vip.github.io/core-x/packages/oauth/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/142vip/core-x.git",
    "directory": "packages/grpc"
  },
  "keywords": [
    "公众号搜：储凡",
    "142vip",
    "@142vip",
    "@142vip/grpc",
    "grpc",
    "proto",
    "proto-loader",
    "grpc-health"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "example-client.js",
    "example-server.js",
    "protos"
  ],
  "authorInfo": {
    "name": "微信公众号：储凡",
    "email": "fairy_vip@2925.com",
    "url": "https://github.com/142vip",
    "homePage": "https://142vip.cn"
  },
  "scripts": {
    "dev:client": "node example-client.js",
    "dev:server": "node example-server.js",
    "test": "npx jest",
    "test:coverage": "npx jest --coverage",
    "build": "unbuild",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.14.0",
    "@grpc/proto-loader": "0.8.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
