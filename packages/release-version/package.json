{
  "name": "@142vip/release-version",
  "version": "0.0.1-alpha.10",
  "description": "版本迭代更新，Git提交信息、打标记并推送到远程",
  "author": {
    "name": "James Messinger",
    "url": "https://jamesmessinger.com"
  },
  "license": "MIT",
  "homepage": "https://142vip.github.io/core-x/packages/release-version/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/142vip/core-x.git",
    "directory": "packages/release-version"
  },
  "keywords": [
    "公众号搜：储凡",
    "142vip",
    "@142vip",
    "@142vip/release-version",
    "bumpx",
    "git"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "bumpx": "./cli.mjs"
  },
  "files": [
    "*.mjs",
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "unbuild",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@142vip/changelog": "workspace:*",
    "@142vip/utils": "workspace:*",
    "c12": "2.0.1",
    "cac": "6.7.14",
    "escalade": "3.1.2",
    "fast-glob": "3.3.2",
    "js-yaml": "4.1.0",
    "prompts": "2.4.2",
    "semver": "7.6.0"
  },
  "devDependencies": {
    "@types/js-yaml": "4.0.9",
    "@types/prompts": "2.4.9",
    "@types/semver": "7.5.8",
    "detect-indent": "7.0.1",
    "detect-newline": "4.0.1",
    "kolorist": "1.8.0",
    "npm-check": "6.0.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
