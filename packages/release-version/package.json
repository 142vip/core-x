  "repository": {
    "type": "git",
    "url": "git+https://github.com/142vip/core-x.git",
    "directory": "packages/release-version"
  },
  "keywords": [
    "公众号搜：储凡",
    "142vip",
    "@142vip",
    "@142vip/release-version",
